meta {
  name: Filter Events - Unknown Query Param
  type: http
  seq: 10
}

get {
  url: {{base_url}}/events?page=1&unknown_param=test
  body: none
  auth: none
}

assert {
  res.status: eq 400
}

tests {
  test("should reject unknown query parameters", function() {
    expect(res.status).to.equal(400);
    expect(res.body.error).to.include("unknown");
  });
}
