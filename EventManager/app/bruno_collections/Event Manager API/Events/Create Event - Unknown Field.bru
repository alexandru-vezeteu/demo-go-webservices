meta {
  name: Create Event - Unknown Field (Strict Binding)
  type: http
  seq: 3
}

post {
  url: {{base_url}}/events
  body: json
  auth: none
}

body:json {
  {
    "owner_id": 1,
    "name": "Tech Conference",
    "unknown_field": "should fail",
    "seats": 100
  }
}

assert {
  res.status: eq 400
}

tests {
  test("should reject unknown fields", function() {
    expect(res.status).to.equal(400);
    expect(res.body.error).to.include("unknown field");
  });
}
