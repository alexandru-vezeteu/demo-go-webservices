meta {
  name: Get Event - Invalid ID Format
  type: http
  seq: 7
}

get {
  url: {{base_url}}/events/abc
  body: none
  auth: none
}

assert {
  res.status: eq 400
}

tests {
  test("should return 400 for invalid ID format", function() {
    expect(res.status).to.equal(400);
    expect(res.body.error).to.include("invalid");
  });
}
