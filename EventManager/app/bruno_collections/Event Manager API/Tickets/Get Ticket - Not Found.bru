meta {
  name: Get Ticket - Not Found
  type: http
  seq: 9
}

get {
  url: {{base_url}}/tickets/00000000-0000-0000-0000-000000000000
  body: none
  auth: none
}

assert {
  res.status: eq 404
}

tests {
  test("should return 404 for non-existent ticket", function() {
    expect(res.status).to.equal(404);
    expect(res.body.error).to.exist;
  });
}
